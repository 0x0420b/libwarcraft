<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Warcraft</RootNamespace>
    <AssemblyName>libwarcraft</AssemblyName>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\libwarcraft.key</AssemblyOriginatorKeyFile>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="ImageSharp">
      <Version>1.0.0-alpha9-00140</Version>
    </PackageReference>
    <PackageReference Include="System.Numerics.Vectors" Version="4.1.2.0" />
    <PackageReference Include="DotNetZip" Version="1.10.1.0" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="App.config" />
  </ItemGroup>
</Project>